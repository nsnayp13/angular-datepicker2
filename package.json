{
  "name": "angular-datepicker",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build angular-datepicker2",
    "build:gh-pages": "ng build --configuration production --base-href https://nsnayp13.github.io/angular-datepicker2/",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "publish:lib": "npm run build:lib && cd dist/angular-datepicker2 && npm publish",
    "version:patch": "cd projects/angular-datepicker2 && npm version patch",
    "version:minor": "cd projects/angular-datepicker2 && npm version minor",
    "version:major": "cd projects/angular-datepicker2 && npm version major"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.0",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.6.0",
    "zone.js": "^0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.0",
    "@angular/cli": "^19.2.0",
    "@angular/compiler-cli": "^19.2.0",
    "@angular/language-service": "^19.2.0",
    "@types/node": "^20.0.0",
    "codecov.io": "^0.1.6",
    "cz-conventional-changelog": "^3.1.0",
    "ng-packagr": "^19.2.0",
    "ts-node": "^10.9.0",
    "typescript": "~5.8.3",
    "semantic-release": "^22.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^9.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
