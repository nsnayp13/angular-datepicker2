{
  "name": "angular-datepicker2",
  "version": "3.4.3",
  "description": "Modern Angular datepicker component with standalone support. Compatible with Angular 16+. Select single, multiple dates or periods. Fully customizable with template support.",
  "main": "bundles/angular-datepicker2.umd.js",
  "module": "fesm2022/angular-datepicker2.mjs",
  "typings": "index.d.ts",
  "exports": {
    ".": {
      "esm2022": "./esm2022/angular-datepicker2.mjs",
      "esm": "./esm2022/angular-datepicker2.mjs"
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "@angular/common": ">=16.0.0",
    "@angular/core": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsnayp13/angular-calendar.git"
  },
  "keywords": [
    "angular",
    "datepicker",
    "calendar",
    "date",
    "picker",
    "component",
    "ui",
    "standalone",
    "angular16",
    "angular17",
    "angular18",
    "angular19",
    "date-range",
    "period-picker",
    "multiple-dates"
  ],
  "author": "Ivanov.N <nikaglichev@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": ["main", "master"],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": ["package.json", "CHANGELOG.md"],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
