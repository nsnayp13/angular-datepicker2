{
  "name": "@nsnayp/angular-datepicker2",
  "version": "3.5.0",
  "description": "Modern Angular datepicker component with standalone support. Compatible with Angular 16+. Select single, multiple dates or periods. Fully customizable with template support.",
  "type": "module",
  "main": "bundles/angular-datepicker2.umd.js",
  "module": "fesm2022/nsnayp-angular-datepicker2.mjs",
  "es2022": "fesm2022/angular-datepicker2.mjs",
  "esm2022": "esm2022/angular-datepicker2.mjs",
  "typings": "index.d.ts",
  "exports": {
    ".": {
      "import": "./fesm2022/angular-datepicker2.mjs",
      "require": "./bundles/angular-datepicker2.umd.js",
      "types": "./index.d.ts",
      "esm2022": "./esm2022/nsnayp-angular-datepicker2.mjs",
      "esm": "./esm2022/nsnayp-angular-datepicker2.mjs",
      "default": "./fesm2022/nsnayp-angular-datepicker2.mjs"
    },
    "./package.json": {
      "default": "./package.json"
    }
  },
  "sideEffects": false,
  "peerDependencies": {
    "@angular/common": ">=16.0.0",
    "@angular/core": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nsnayp13/angular-datepicker2.git"
  },
  "homepage": "https://github.com/nsnayp13/angular-datepicker2#readme",
  "bugs": {
    "url": "https://github.com/nsnayp13/angular-datepicker2/issues"
  },
  "keywords": [
    "angular",
    "datepicker",
    "calendar",
    "date",
    "picker",
    "component",
    "ui",
    "standalone",
    "angular16",
    "angular17",
    "angular18",
    "angular19",
    "date-range",
    "period-picker",
    "multiple-dates"
  ],
  "author": "Ivanov.N <nikaglichev@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}